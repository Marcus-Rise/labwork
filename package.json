  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-modern": "vue-cli-service build --modern",
    "test:unit": "vue-cli-service test:unit",
    "package-dev": "set NODE_ENV=development && tsc  index.ts --outDir ./dist --module commonjs && electron ./dist/index.js",
    "package": "set NODE_ENV=production && tsc  index.ts --outDir ./dist --module commonjs && electron-packager --platform=linux,win32,darwin --arch=x64 ./dist --overwrite --asar --out=desktop",
    "create-installer-debian": "electron-installer-debian --src desktop/net-labwork-linux-x64 --arch=x64 --dest desktop/linux-setup",
    "create-installer-win": "electron-installer-windows --src desktop/net-labwork-win32-x64 --arch=x64 --dest desktop/win-setup"
  },
  "dependencies": {
    "chart.js": "^2.8.0",
    "core-js": "^2.6.5",
    "material-design-icons-iconfont": "^3.0.3",
    "pdfvuer": "^1.3.0",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
