{
  "name": "net-labwork",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-modern": "vue-cli-service build --modern",
    "test:unit": "vue-cli-service test:unit",
    "package-dev": "set NODE_ENV=development && tsc  index.ts --outDir ./dist --module commonjs && electron ./dist/index.js",
    "package-win": "set NODE_ENV=production && tsc  index.ts --outDir ./dist --module commonjs && electron-packager ./dist packaged --platform=win32 --arch=x64 --overwrite --asar",
    "package-linux": "set NODE_ENV=production && tsc  index.ts --outDir ./dist --module commonjs && electron-packager ./dist packaged --prune=true --platform=linux --arch=x64 --overwrite --asar"
  },
  "dependencies": {
    "core-js": "^2.6.5",
    "material-design-icons-iconfont": "^3.0.3",
    "register-service-worker": "^1.6.2",
    "roboto-fontface": "*",
    "vue": "^2.6.10",
    "vue-class-component": "^7.0.2",
    "vue-property-decorator": "^8.1.0",
    "vue-router": "^3.0.3",
    "vuetify": "^1.5.5",
    "vuex": "^3.0.1",
    "vuex-class": "^0.3.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.4",
    "@types/electron": "^1.6.10",
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-pwa": "^3.6.0",
    "@vue/cli-plugin-typescript": "^3.6.0",
    "@vue/cli-plugin-unit-jest": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "@vue/test-utils": "1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "electron": "^4.0.6",
    "electron-packager": "^13.1.0",
    "sass": "^1.18.0",
    "sass-loader": "^7.1.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-jest": "^23.0.0",
    "typescript": "^3.4.3",
    "vue-cli-plugin-vuetify": "^0.5.0",
    "vue-template-compiler": "^2.5.21",
    "vuetify-loader": "^1.0.5",
    "vuetify-scss": "^1.3.5",
    "vuex-module-decorators": "^0.9.8",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0"
  }
}
